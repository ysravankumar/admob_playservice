#!/usr/bin/env mkb
# Builder mkb file for the gpgadmob extension on android
platform LINUX

buildtype (SCONS, ARM)

define S3E_ANDROID

options
{
    dll
    output-name="lib/android/libgpgadmob.so"
    output-name-debug="lib/android/libgpgadmob_d.so"
}
define S3E_BUILD_EXT

subproject gpgadmob_build
