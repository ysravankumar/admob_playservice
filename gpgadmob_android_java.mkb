#!/usr/bin/env mkb
# Builder mkb file for the java portion of the gpgadmob extension on android
platform JAVA

files
{
    (source/android)
    gpgadmob.java
}
librarypath "lib/android/google-play-services.jar"
librarypath "$MARMALADE_ROOT/s3e/deploy/plugins/android/android.jar"
librarypath "$MARMALADE_ROOT/s3e/loader/android/s3e_release.jar"

option output-name=lib/android/gpgadmob.jar
