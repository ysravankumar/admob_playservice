#!/usr/bin/env mkb
# Builder mkb file for the gpgadmob extension on wp8
platform WP8LOADER

options
{
    buildenv=vc11
    buildenv-toolset=wp8
    dll
    #arm
    output-name="lib/wp8/$(PlatformTarget)/$(Configuration)/gpgadmobExtension.dll"
}

subproject gpgadmob_wp8_cs
define S3E_BUILD_EXT

subproject gpgadmob_build
